
class Trabajo {
	var gasto
	var contratista
	var fecha

	method fecha() {
		return fecha
	}

	method fecha(_fecha) {
		fecha = _fecha
	}

	method contratista(_contratista) {
		contratista = _contratista
	}

	method contratista() {
		return contratista
	}

	method gasto() {
		return gasto
	}

	method gasto(_gasto) {
		gasto = _gasto
	}
} 